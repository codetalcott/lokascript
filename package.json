{
  "name": "hyperfixi",
  "version": "0.1.0",
  "description": "Unified fetch syntax for _hyperscript + fixi.js integration",
  "type": "module",
  "main": "dist/hyperscript-fixi.mjs",
  "browser": "dist/hyperscript-fixi.min.js",
  "types": "dist/hyperscript-fixi.d.ts",
  "exports": {
    ".": {
      "import": "./dist/hyperscript-fixi.mjs",
      "require": "./dist/hyperscript-fixi.min.js",
      "types": "./dist/hyperscript-fixi.d.ts"
    }
  },
  "files": [
    "dist/",
    "src/",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:browser": "open test.html",
    "test:simple": "open test-simple.html",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "size": "npm run size:analysis",
    "size:uncompressed": "wc -c dist/hyperscript-fixi.mjs",
    "size:gzipped": "gzip -c dist/hyperscript-fixi.mjs | wc -c",
    "size:minified": "wc -c dist/hyperscript-fixi.min.js",
    "size:analysis": "echo 'Size Analysis:' && echo 'ES Module: '$(wc -c < dist/hyperscript-fixi.mjs)' bytes' && echo 'Minified UMD: '$(wc -c < dist/hyperscript-fixi.min.js)' bytes' && echo 'Gzipped: '$(gzip -c dist/hyperscript-fixi.mjs | wc -c)' bytes'",
    "generate": "tsx src/generator/generate-from-lsp.ts",
    "db:extract": "tsx src/generator/extract-database.ts",
    "benchmark": "tsx src/benchmark/run-benchmarks.ts"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "better-sqlite3": "^9.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "happy-dom": "^12.0.0",
    "prettier": "^3.0.0",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.8.1",
    "tsx": "^4.0.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3",
    "zod": "^3.22.0"
  },
  "keywords": [
    "hyperscript",
    "fixi",
    "fetch",
    "ajax",
    "hypermedia",
    "minimalist",
    "dom",
    "reactive"
  ],
  "author": "HyperFixi Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperfixi/hyperfixi.git"
  },
  "bugs": {
    "url": "https://github.com/hyperfixi/hyperfixi/issues"
  },
  "homepage": "https://github.com/hyperfixi/hyperfixi#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
