{
  "name": "@hyperfixi/client",
  "version": "0.1.0",
  "description": "JavaScript/TypeScript client library for HyperFixi server-side hyperscript compilation with Express and Elysia support",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "prepublishOnly": "npm run build",
    "dev": "npm run build:watch"
  },
  "keywords": [
    "hyperscript",
    "server-side",
    "compilation",
    "express",
    "elysia",
    "bun",
    "middleware",
    "typescript",
    "hyperfixi"
  ],
  "author": "HyperFixi Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperfixi/hyperfixi.git",
    "directory": "clients/js-client"
  },
  "dependencies": {
    "axios": "^1.12.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^8.54.0",
    "express": "^4.22.1",
    "rollup": "^4.6.1",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "express": "^4.18.0",
    "elysia": "^0.8.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "elysia": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./express": {
      "import": "./dist/express.mjs",
      "require": "./dist/express.js",
      "types": "./dist/express.d.ts"
    },
    "./elysia": {
      "import": "./dist/elysia.mjs",
      "require": "./dist/elysia.js",
      "types": "./dist/elysia.d.ts"
    }
  }
}