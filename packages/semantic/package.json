{
  "name": "@hyperfixi/semantic",
  "version": "0.1.0",
  "description": "Semantic-first multilingual parsing for Hyperscript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "browser": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "default": "./dist/browser.global.js"
    },
    "./browser/en": {
      "default": "./dist/browser-en.en.global.js"
    },
    "./browser/es-en": {
      "default": "./dist/browser-es-en.es-en.global.js"
    },
    "./languages": {
      "types": "./dist/languages/index.d.ts",
      "import": "./dist/languages/index.mjs"
    },
    "./languages/en": {
      "types": "./dist/languages/en.d.ts",
      "import": "./dist/languages/en.mjs"
    },
    "./languages/es": {
      "types": "./dist/languages/es.d.ts",
      "import": "./dist/languages/es.mjs"
    },
    "./languages/ja": {
      "types": "./dist/languages/ja.d.ts",
      "import": "./dist/languages/ja.mjs"
    },
    "./languages/ar": {
      "types": "./dist/languages/ar.d.ts",
      "import": "./dist/languages/ar.mjs"
    },
    "./languages/ko": {
      "types": "./dist/languages/ko.d.ts",
      "import": "./dist/languages/ko.mjs"
    },
    "./languages/zh": {
      "types": "./dist/languages/zh.d.ts",
      "import": "./dist/languages/zh.mjs"
    },
    "./languages/tr": {
      "types": "./dist/languages/tr.d.ts",
      "import": "./dist/languages/tr.mjs"
    },
    "./languages/pt": {
      "types": "./dist/languages/pt.d.ts",
      "import": "./dist/languages/pt.mjs"
    },
    "./languages/fr": {
      "types": "./dist/languages/fr.d.ts",
      "import": "./dist/languages/fr.mjs"
    },
    "./languages/de": {
      "types": "./dist/languages/de.d.ts",
      "import": "./dist/languages/de.mjs"
    },
    "./languages/id": {
      "types": "./dist/languages/id.d.ts",
      "import": "./dist/languages/id.mjs"
    },
    "./languages/qu": {
      "types": "./dist/languages/qu.d.ts",
      "import": "./dist/languages/qu.mjs"
    },
    "./languages/sw": {
      "types": "./dist/languages/sw.d.ts",
      "import": "./dist/languages/sw.mjs"
    },
    "./explicit": {
      "types": "./dist/explicit/index.d.ts",
      "import": "./dist/explicit/index.mjs",
      "require": "./dist/explicit/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:browser": "tsup src/browser.ts --format iife --global-name HyperFixiSemantic --minify",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": [
    "./src/languages/*.ts",
    "./dist/languages/*.mjs",
    "./src/patterns/index.ts"
  ],
  "keywords": [
    "hyperscript",
    "semantic",
    "multilingual",
    "i18n",
    "parser",
    "natural-language"
  ],
  "author": "Hyperfixi Team",
  "license": "MIT",
  "dependencies": {
    "@hyperfixi/i18n": "*",
    "@hyperfixi/expression-parser": "*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.0"
  },
  "files": [
    "dist",
    "src"
  ]
}
