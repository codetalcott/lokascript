{
  "name": "@lokascript/semantic",
  "version": "1.2.0",
  "description": "Semantic multilingual parser for hyperscript (23 languages)",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./browser": {
      "types": "./dist/index.d.ts",
      "default": "./dist/browser.global.js"
    },
    "./browser/en": {
      "default": "./dist/browser-en.en.global.js"
    },
    "./browser/es-en": {
      "default": "./dist/browser-es-en.es-en.global.js"
    },
    "./browser/es": {
      "default": "./dist/browser-es.es.global.js"
    },
    "./browser/tr": {
      "default": "./dist/browser-tr.tr.global.js"
    },
    "./browser/western": {
      "default": "./dist/browser-western.western.global.js"
    },
    "./browser/east-asian": {
      "default": "./dist/browser-east-asian.east-asian.global.js"
    },
    "./browser/priority": {
      "default": "./dist/browser-priority.priority.global.js"
    },
    "./browser/lazy": {
      "default": "./dist/browser-lazy.lazy.global.js"
    },
    "./languages/en": {
      "types": "./dist/languages/en.d.ts",
      "import": "./dist/languages/en.js"
    },
    "./languages/es": {
      "types": "./dist/languages/es.d.ts",
      "import": "./dist/languages/es.js"
    },
    "./languages/ja": {
      "types": "./dist/languages/ja.d.ts",
      "import": "./dist/languages/ja.js"
    },
    "./languages/ar": {
      "types": "./dist/languages/ar.d.ts",
      "import": "./dist/languages/ar.js"
    },
    "./languages/ko": {
      "types": "./dist/languages/ko.d.ts",
      "import": "./dist/languages/ko.js"
    },
    "./languages/zh": {
      "types": "./dist/languages/zh.d.ts",
      "import": "./dist/languages/zh.js"
    },
    "./languages/tr": {
      "types": "./dist/languages/tr.d.ts",
      "import": "./dist/languages/tr.js"
    },
    "./languages/pt": {
      "types": "./dist/languages/pt.d.ts",
      "import": "./dist/languages/pt.js"
    },
    "./languages/fr": {
      "types": "./dist/languages/fr.d.ts",
      "import": "./dist/languages/fr.js"
    },
    "./languages/de": {
      "types": "./dist/languages/de.d.ts",
      "import": "./dist/languages/de.js"
    },
    "./languages/id": {
      "types": "./dist/languages/id.d.ts",
      "import": "./dist/languages/id.js"
    },
    "./languages/qu": {
      "types": "./dist/languages/qu.d.ts",
      "import": "./dist/languages/qu.js"
    },
    "./languages/sw": {
      "types": "./dist/languages/sw.d.ts",
      "import": "./dist/languages/sw.js"
    },
    "./browser/ja": {
      "default": "./dist/browser-ja.ja.global.js"
    },
    "./browser/ar": {
      "default": "./dist/browser-ar.ar.global.js"
    },
    "./browser/ko": {
      "default": "./dist/browser-ko.ko.global.js"
    },
    "./browser/zh": {
      "default": "./dist/browser-zh.zh.global.js"
    },
    "./browser/pt": {
      "default": "./dist/browser-pt.pt.global.js"
    },
    "./browser/fr": {
      "default": "./dist/browser-fr.fr.global.js"
    },
    "./browser/de": {
      "default": "./dist/browser-de.de.global.js"
    },
    "./browser/id": {
      "default": "./dist/browser-id.id.global.js"
    },
    "./browser/qu": {
      "default": "./dist/browser-qu.qu.global.js"
    },
    "./browser/sw": {
      "default": "./dist/browser-sw.sw.global.js"
    },
    "./browser/core": {
      "default": "./dist/browser-core.core.global.js"
    },
    "./browser/en-tr": {
      "default": "./dist/browser-en-tr.en-tr.global.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:browser": "tsup src/browser.ts --format iife --global-name LokaScriptSemantic --minify",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "add-language": "npx tsx scripts/add-language.ts",
    "update-keywords": "npx tsx scripts/update-keywords.ts",
    "generate:indexes": "npx tsx scripts/generate-indexes.ts",
    "prebuild": "npm run generate:indexes",
    "benchmark": "npx tsx scripts/benchmark-all.ts",
    "benchmark:json": "npx tsx scripts/benchmark-all.ts --json",
    "benchmark:save": "npx tsx scripts/benchmark-all.ts --save",
    "benchmark:ci": "npx tsx scripts/benchmark-all.ts --save --compare",
    "benchmark:lookup": "npx tsx scripts/benchmark-tokenizer-lookup.ts"
  },
  "sideEffects": [
    "./src/languages/*.ts",
    "./dist/languages/*.js",
    "./src/patterns/index.ts"
  ],
  "keywords": [
    "hyperscript",
    "semantic",
    "multilingual",
    "i18n",
    "parser",
    "natural-language"
  ],
  "author": "Hyperfixi Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^4.0.17",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.0"
  },
  "files": [
    "dist",
    "src",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codetalcott/lokascript.git",
    "directory": "packages/semantic"
  },
  "bugs": {
    "url": "https://github.com/codetalcott/lokascript/issues"
  },
  "homepage": "https://github.com/codetalcott/lokascript/tree/main/packages/semantic#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
