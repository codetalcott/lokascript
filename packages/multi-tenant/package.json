{
  "name": "@lokascript/multi-tenant",
  "version": "1.0.0-rc.1",
  "private": true,
  "description": "Multi-tenant behavior customization and tenant isolation for HyperFixi applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./tenant-manager": {
      "types": "./dist/tenant-manager.d.ts",
      "import": "./dist/tenant-manager.mjs",
      "require": "./dist/tenant-manager.js"
    },
    "./isolation": {
      "types": "./dist/isolation.d.ts",
      "import": "./dist/isolation.mjs",
      "require": "./dist/isolation.js"
    },
    "./customization": {
      "types": "./dist/customization.d.ts",
      "import": "./dist/customization.mjs",
      "require": "./dist/customization.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "import": "./dist/middleware.mjs",
      "require": "./dist/middleware.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix"
  },
  "keywords": [
    "hyperscript",
    "multi-tenant",
    "tenant-isolation",
    "customization",
    "saas",
    "tenant-context",
    "behavior-isolation",
    "hyperfixi"
  ],
  "author": "HyperFixi Team",
  "license": "MIT",
  "dependencies": {
    "@lokascript/core": "*",
    "esbuild": "0.25.11",
    "happy-dom": "20.0.6",
    "vite": "^7.1.12"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^4.0.0",
    "eslint": "^8.0.0",
    "jsdom": "^23.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  }
}
