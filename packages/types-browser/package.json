{
  "name": "@lokascript/types-browser",
  "version": "1.0.0",
  "description": "TypeScript type definitions for HyperFixi browser globals",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "src/",
