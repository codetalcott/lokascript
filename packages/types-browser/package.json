  "keywords": [
    "hyperfixi",
    "types",
    "typescript",
    "browser",
    "globals"
  ],
  "author": "HyperFixi Contributors",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
