{
  "name": "@lokascript/behaviors",
  "version": "1.3.0",
  "description": "Reusable hyperscript behaviors for LokaScript",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./registry": {
      "types": "./dist/registry.d.ts",
      "import": "./dist/registry.js",
      "require": "./dist/registry.cjs"
    },
    "./loaders": {
      "types": "./dist/loaders.d.ts",
      "import": "./dist/loaders.js",
      "require": "./dist/loaders.cjs"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.js",
      "require": "./dist/schemas/index.cjs"
    },
    "./draggable": {
      "types": "./dist/behaviors/draggable.d.ts",
      "import": "./dist/behaviors/draggable.js",
      "require": "./dist/behaviors/draggable.cjs"
    },
    "./removable": {
      "types": "./dist/behaviors/removable.d.ts",
      "import": "./dist/behaviors/removable.js",
      "require": "./dist/behaviors/removable.cjs"
    },
    "./toggleable": {
      "types": "./dist/behaviors/toggleable.d.ts",
      "import": "./dist/behaviors/toggleable.js",
      "require": "./dist/behaviors/toggleable.cjs"
    },
    "./sortable": {
      "types": "./dist/behaviors/sortable.d.ts",
      "import": "./dist/behaviors/sortable.js",
      "require": "./dist/behaviors/sortable.cjs"
    },
    "./resizable": {
      "types": "./dist/behaviors/resizable.d.ts",
      "import": "./dist/behaviors/resizable.js",
      "require": "./dist/behaviors/resizable.cjs"
    },
    "./draggable.browser.js": "./dist/draggable.browser.global.js",
    "./removable.browser.js": "./dist/removable.browser.global.js",
    "./toggleable.browser.js": "./dist/toggleable.browser.global.js",
    "./sortable.browser.js": "./dist/sortable.browser.global.js",
    "./resizable.browser.js": "./dist/resizable.browser.global.js",
    "./all.browser.js": "./dist/all.browser.global.js",
    "./core.browser.js": "./dist/core.browser.global.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "generate": "tsx scripts/generate.ts",
    "prebuild": "npm run generate",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "@lokascript/core": "*"
  },
  "devDependencies": {
    "@lokascript/core": "*",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.0"
  },
  "keywords": [
    "hyperscript",
    "hyperfixi",
    "behaviors",
    "draggable",
    "sortable",
    "resizable",
    "removable",
    "toggleable",
    "ui-components"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codetalcott/lokascript.git",
    "directory": "packages/behaviors"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/codetalcott/lokascript/issues"
  },
  "homepage": "https://github.com/codetalcott/lokascript/tree/main/packages/behaviors#readme"
}
