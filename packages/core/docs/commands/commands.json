        "fetch \"/api/save\" with method:\"POST\"",
        "fetch \"/api/upload\" with { method:\"POST\", body:formData }",
        "fetch \"/partial.html\" as html",
        "fetch \"/slow\" with timeout:5000"
      ],
      "category": "async",
      "sideEffects": ["network", "event-dispatching"]
    },
    "transition": {
