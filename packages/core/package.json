{
  "name": "@hyperfixi/core",
  "version": "1.0.0",
  "description": "Pure hyperscript expression evaluation engine with TypeScript support",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./parser": {
      "types": "./dist/parser/index.d.ts",
      "import": "./dist/parser/index.mjs",
      "require": "./dist/parser/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.mjs",
      "require": "./dist/runtime/index.js"
    }
  },
  "files": [
    "dist/",
    "src/",
    "docs/",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:browser": "rollup -c rollup.browser.config.mjs",
    "build:browser:dev": "rollup -c rollup.browser.dev.config.mjs",
    "build:browser:prod": "rollup -c rollup.browser.prod.config.mjs",
    "build:browser:llm": "rollup -c rollup.browser.llm.config.mjs",
    "build:browser:minimal": "rollup -c rollup.browser-minimal.config.mjs",
    "build:browser:standard": "rollup -c rollup.browser-standard.config.mjs",
    "build:browser:all": "npm run build:browser:dev && npm run build:browser:prod && npm run build:browser:llm && npm run build:browser:minimal && npm run build:browser:standard",
    "build:browser:esm": "rollup -c rollup.browser-esm.config.mjs",
    "build:browser:esm:minimal": "rollup -c rollup.browser-minimal-esm.config.mjs",
    "build:browser:esm:standard": "rollup -c rollup.browser-standard-esm.config.mjs",
    "build:browser:esm:all": "npm run build:browser:esm && npm run build:browser:esm:minimal && npm run build:browser:esm:standard",
    "build:browser:modular": "rollup -c rollup.browser-modular.config.mjs",
    "dev": "rollup -c -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:browser": "npm run build:browser && npx playwright test",
    "test:feedback": "node scripts/test-feedback.mjs",
    "test:feedback:json": "node scripts/test-feedback.mjs --json",
    "test:feedback:md": "node scripts/test-feedback.mjs --markdown",
    "test:feedback:verbose": "node scripts/test-feedback.mjs --verbose",
    "test:comprehensive": "npm run build:browser && npm run test:feedback",
    "test:quick": "npm run build:browser && node scripts/test-feedback.mjs --quick",
    "test:cookbook": "node validate-cookbook-demos.mjs",
    "test:prod": "NODE_ENV=production node production-test-runner.js --quiet",
    "test:prod:verbose": "node production-test-runner.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "analyze:usage": "node scripts/analyze-usage.mjs",
    "size": "npm run size:analysis",
    "size:analysis": "echo 'Size Analysis:' && echo 'ES Module: '$(wc -c < dist/index.mjs)' bytes' && echo 'CJS Module: '$(wc -c < dist/index.js)' bytes' && echo 'Gzipped: '$(gzip -c dist/index.mjs | wc -c)' bytes'"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "better-sqlite3": "^9.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "happy-dom": "^20.0.6",
    "http-server": "^14.1.1",
    "prettier": "^3.0.0",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "tslib": "^2.8.1",
    "tsx": "^4.0.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "zod": "^3.25.76"
  },
  "keywords": [
    "hyperscript",
    "parser",
    "expressions",
    "dom",
    "typescript",
    "ast"
  ],
  "author": "HyperFixi Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperfixi/hyperfixi.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/hyperfixi/hyperfixi/issues"
  },
  "homepage": "https://github.com/hyperfixi/hyperfixi/tree/main/packages/core#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "esbuild": "0.25.11",
    "jsdom": "^26.1.0",
    "morphlex": "^1.0.5",
    "vite": "^7.1.12"
  },
  "directories": {
    "doc": "docs"
  }
}
