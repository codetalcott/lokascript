{
  "name": "@hyperfixi/analytics",
  "version": "0.1.0",
  "description": "Behavior analytics and instrumentation for HyperFixi applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./tracker": {
      "import": "./dist/tracker.mjs",
      "require": "./dist/tracker.js",
      "types": "./dist/tracker.d.ts"
    },
    "./collector": {
      "import": "./dist/collector.mjs",
      "require": "./dist/collector.js",
      "types": "./dist/collector.d.ts"
    },
    "./metrics": {
      "import": "./dist/metrics.mjs",
      "require": "./dist/metrics.js",
      "types": "./dist/metrics.d.ts"
    },
    "./dashboard": {
      "import": "./dist/dashboard.mjs",
      "require": "./dist/dashboard.js",
      "types": "./dist/dashboard.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix"
  },
  "keywords": [
    "hyperscript",
    "analytics",
    "behavior-tracking",
    "instrumentation",
    "metrics",
    "performance",
    "user-behavior",
    "dashboard",
    "hyperfixi"
  ],
  "author": "HyperFixi Team",
  "license": "MIT",
  "dependencies": {
    "@hyperfixi/core": "*",
    "happy-dom": "20.0.6",
    "vite": "7.1.10"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.0.0",
    "jsdom": "^23.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  }
}
