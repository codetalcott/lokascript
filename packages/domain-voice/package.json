{
  "name": "@lokascript/domain-voice",
  "version": "2.1.0",
  "description": "Multilingual voice/accessibility commands â€” 14 commands in 8 languages for speech-controlled web interaction",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly --outDir dist --noEmit false",
    "test": "vitest",
    "test:run": "vitest run",
    "test:check": "vitest run --reporter=dot 2>&1 | tail -5",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@lokascript/framework": "*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.0"
  },
  "files": [
    "dist",
    "src",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codetalcott/hyperfixi.git",
    "directory": "packages/domain-voice"
  }
}
