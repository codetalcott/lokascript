{
  "name": "@lokascript/ast-toolkit",
  "private": true,
  "version": "2.1.0",
  "deprecated": "Consolidated into @hyperfixi/core/ast-utils. Use '@hyperfixi/core/ast-utils' instead.",
  "description": "DEPRECATED: Advanced AST toolkit for hyperscript â€” consolidated into @hyperfixi/core/ast-utils",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./visitor": {
      "types": "./dist/visitor/index.d.ts",
      "import": "./dist/visitor/index.js"
    },
    "./query": {
      "types": "./dist/query/index.d.ts",
      "import": "./dist/query/index.js"
    },
    "./analyzer": {
      "types": "./dist/analyzer/index.d.ts",
      "import": "./dist/analyzer/index.js"
    },
    "./transformer": {
      "types": "./dist/transformer/index.d.ts",
      "import": "./dist/transformer/index.js"
    },
    "./generator": {
      "types": "./dist/generator/index.d.ts",
      "import": "./dist/generator/index.js"
    },
    "./ai": {
      "types": "./dist/ai/index.d.ts",
      "import": "./dist/ai/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "dev": "tsc --watch",
    "test:check": "vitest run --reporter=dot 2>&1 | tail -5"
  },
  "keywords": [
    "ast",
    "hyperscript",
    "parser",
