{
  "name": "@lokascript/server-integration",
  "version": "1.3.0",
  "private": true,
  "description": "Server-side hyperscript integration with multi-language support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:run": "vitest --run",
    "dev": "tsx src/server/index.ts",
    "start": "node dist/server/index.js"
  },
  "dependencies": {
    "@lokascript/core": "*",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "esbuild": "0.25.11",
    "express": "^4.22.1",
    "helmet": "^7.2.0",
    "node-cache": "^5.1.2",
    "pg": "^8.16.3",
    "stripe": "^17.7.0",
    "vite": "^7.3.0"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/node": "^20.19.27",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "happy-dom": "^20.0.11",
    "supertest": "^6.3.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "hyperscript",
    "server-side",
    "template",
    "integration",
    "compilation"
  ],
  "author": "LokaScript Contributors",
  "license": "MIT"
}
